#Region "H_OUTKAEDI_DTL_NCGO_NEW 取得 (セミEDI時・出荷赤伝・取消抽出)"

    Private Const SQL_SELECT_OUTKAEDI_DTL_NCGO_NEW_CANCEL As String = "  SELECT                        " & vbNewLine _
                                    & "   CHKNCG.OUTKA_DENP_NO     AS  OUTKA_DENP_NO                   " & vbNewLine _
                                    & "  ,CHKNCG.EDI_CTL_NO        AS  EDI_CTL_NO                      " & vbNewLine _
                                    & " FROM                                                           " & vbNewLine _
                                    & " $LM_TRN$..H_OUTKAEDI_DTL_NCGO_NEW OUTDTLNCG                    " & vbNewLine _
                                    & " --取消対象抽出なのでINNER JOINでOK                             " & vbNewLine _
                                    & " INNER JOIN  $LM_TRN$..H_OUTKAEDI_DTL_NCGO_NEW CHKNCG           " & vbNewLine _
                                    & "   ON  CHKNCG.NRS_BR_CD      =  @NRS_BR_CD                      " & vbNewLine _
                                    & "  AND CHKNCG.CUST_CD_L       =  @CUST_CD_L                      " & vbNewLine _
                                    & "  AND CHKNCG.CUST_CD_M       =  @CUST_CD_M                      " & vbNewLine _
                                    & "  AND ((CHKNCG.SYS_ENT_DATE   <> OUTDTLNCG.SYS_ENT_DATE         " & vbNewLine _
                                    & "       OR CHKNCG.SYS_ENT_TIME  <> OUTDTLNCG.SYS_ENT_TIME)       " & vbNewLine _
                                    & "     OR (CHKNCG.SYS_ENT_DATE  = OUTDTLNCG.SYS_ENT_DATE          " & vbNewLine _
                                    & "       AND CHKNCG.SYS_ENT_TIME = OUTDTLNCG.SYS_ENT_TIME         " & vbNewLine _
                                    & "       AND CHKNCG.REC_NO < OUTDTLNCG.REC_NO ))                  " & vbNewLine _
                                    & "  AND CHKNCG.DEL_KB         = '0'                               " & vbNewLine _
                                    & "  AND CHKNCG.OUTKA_DENP_NO  = OUTDTLNCG.OUTKA_DENP_NO           " & vbNewLine _
                                    & "  AND RIGHT(CHKNCG.OUTKA_CTL_NO,8)   = '00000000'  --出荷未登録 " & vbNewLine _
                                    & " WHERE                                                          " & vbNewLine _
                                    & "       OUTDTLNCG.NRS_BR_CD      =  @NRS_BR_CD                   " & vbNewLine _
                                    & "   AND OUTDTLNCG.CUST_CD_L      =  @CUST_CD_L                   " & vbNewLine _
                                    & "   AND OUTDTLNCG.CUST_CD_M      =  @CUST_CD_M                   " & vbNewLine _
                                    & "   AND OUTDTLNCG.SYS_ENT_DATE   =  @SYS_ENT_DATE                " & vbNewLine _
                                    & "   AND OUTDTLNCG.SYS_ENT_TIME   =  @SYS_ENT_TIME                " & vbNewLine _
                                    & "   AND OUTDTLNCG.DEL_KB         =  '3'                          " & vbNewLine _
                                    & "  GROUP BY CHKNCG.OUTKA_DENP_NO   ,CHKNCG.EDI_CTL_NO            " & vbNewLine

#End Region ' "H_OUTKAEDI_DTL_NCGO_NEW 取得 (セミEDI時・出荷赤伝・取消抽出)"

