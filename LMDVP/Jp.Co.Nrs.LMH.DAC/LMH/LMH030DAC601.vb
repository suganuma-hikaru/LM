    Private Const SQL_SELECT_FOR_OUTKAEDI_L_FROM_OUTKAEDI_DTL_NCGO_NEW As String = " SELECT  DISTINCT                  " & vbNewLine _
                                                        & "--     CASE WHEN OUTDTLNCG.INPUT_KBN   = '4'    --取消(出荷済み)  " & vbNewLine _
