#Region "H_UNSOEDI_DTL_NCGO 取得 (セミEDI時・輸送赤伝・取消抽出)"

    Private Const SQL_SELECT_UNSOEDI_DTL_NCGO_CANCEL As String = "  SELECT                        " & vbNewLine _
                                    & "   CHKNCG.OUTKA_DENP_NO     AS  OUTKA_DENP_NO                   " & vbNewLine _
                                    & "  ,CHKNCG.EDI_CTL_NO        AS  EDI_CTL_NO                      " & vbNewLine _
                                    & "  ,CHKNCG.YUSO_COMP_CD      AS  YUSO_COMP_CD  --add 2017/02/02  " & vbNewLine _
                                    & "  ,CHKNCG.HOKAN_BASYO       AS  HOKAN_BASYO  --add 2017/02/02  " & vbNewLine _
                                    & " FROM                                                           " & vbNewLine _
                                    & " $LM_TRN$..H_UNSOEDI_DTL_NCGO UNSODTL                           " & vbNewLine _
                                    & " --取消対象抽出なのでINNER JOINでOK                             " & vbNewLine _
                                    & " INNER JOIN  $LM_TRN$..H_UNSOEDI_DTL_NCGO CHKNCG                " & vbNewLine _
                                    & "   ON  CHKNCG.NRS_BR_CD      =  @NRS_BR_CD                      " & vbNewLine _
                                    & "  AND CHKNCG.CUST_CD_L       =  @CUST_CD_L                      " & vbNewLine _
                                    & "  AND CHKNCG.CUST_CD_M       =  @CUST_CD_M                      " & vbNewLine _
                                    & "  AND ((CHKNCG.SYS_ENT_DATE   <> UNSODTL.SYS_ENT_DATE           " & vbNewLine _
                                    & "       OR CHKNCG.SYS_ENT_TIME  <> UNSODTL.SYS_ENT_TIME)         " & vbNewLine _
                                    & "    OR (CHKNCG.SYS_ENT_DATE  = UNSODTL.SYS_ENT_DATE             " & vbNewLine _
                                    & "     AND CHKNCG.SYS_ENT_TIME = UNSODTL.SYS_ENT_TIME             " & vbNewLine _
                                    & "     AND CHKNCG.REC_NO < UNSODTL.REC_NO ))                      " & vbNewLine _
                                    & "  AND CHKNCG.DEL_KB         = '0'                               " & vbNewLine _
                                    & "  AND CHKNCG.OUTKA_DENP_NO  = UNSODTL.OUTKA_DENP_NO             " & vbNewLine _
                                    & "  AND CHKNCG.OUTKA_DENP_DTL_NO  = UNSODTL.OUTKA_DENP_DTL_NO  --ADD 2017/02/01             " & vbNewLine _
                                    & "  AND RIGHT(CHKNCG.OUTKA_CTL_NO,8)   = '00000000'  --出荷未登録 " & vbNewLine _
                                    & " WHERE                                                          " & vbNewLine _
                                    & "       UNSODTL.NRS_BR_CD      =  @NRS_BR_CD                   " & vbNewLine _
                                    & "   AND UNSODTL.CUST_CD_L      =  @CUST_CD_L                   " & vbNewLine _
                                    & "   AND UNSODTL.CUST_CD_M      =  @CUST_CD_M                   " & vbNewLine _
                                    & "   AND UNSODTL.SYS_ENT_DATE   =  @SYS_ENT_DATE                " & vbNewLine _
                                    & "   AND UNSODTL.SYS_ENT_TIME   =  @SYS_ENT_TIME                " & vbNewLine _
                                    & "   AND UNSODTL.DEL_KB         =  '3'                          " & vbNewLine _
                                    & "  GROUP BY CHKNCG.OUTKA_DENP_NO  ,CHKNCG.EDI_CTL_NO           " & vbNewLine _
                                    & "          ,CHKNCG.YUSO_COMP_CD   ,CHKNCG.HOKAN_BASYO          " & vbNewLine

