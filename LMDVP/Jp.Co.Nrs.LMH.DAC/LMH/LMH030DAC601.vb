        Dim isSelectOutka As Boolean = False
        If ds.Tables("LMH030_OUTKAEDI_DTL_NCGO_NEW").Rows.Count() > 0 Then
            isSelectOutka = True
        End If

        ' SQL格納変数の初期化
        Me._StrSql = New StringBuilder()

