        & "  WHERE DTL.NRS_BR_CD       = @NRS_BR_CD                                                                         " & vbNewLine _
        & "    AND DTL.SYS_DEL_FLG     = '0'                                                                                " & vbNewLine _
        & "    AND DTL.DEL_KB          = '0'                                                                                " & vbNewLine _
        & "    AND DTL.OUTKA_CTL_NO   <> 'N00000000'                                                                        " & vbNewLine _
        & "    AND DTL.OUTKA_CTL_NO   <> '000000000'                                                                        " & vbNewLine _
        & "    AND CL.NRS_BR_CD IS NOT NULL                                                                                 " & vbNewLine _
        & "    AND DTL.OUTKA_CTL_NO   <> ''                                                                                 " & vbNewLine _
        & "    AND HOKAN.KBN_CD IS NOT NULL                                                                                 " & vbNewLine

