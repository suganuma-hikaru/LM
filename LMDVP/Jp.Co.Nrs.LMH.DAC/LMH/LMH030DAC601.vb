        & "    AND DTL.SYS_DEL_FLG     = '0'                                                                                " & vbNewLine _
        & "    AND DTL.DEL_KB          = '0'                                                                                " & vbNewLine _
        & " --DEL 2018/07/12   AND UNSO.EDI_CTL_NO IS NULL                                                                                  " & vbNewLine _
        & " --ADD 2018/07/12  Start                                                                                         " & vbNewLine _
        & "     AND (UNSO.EDI_CTL_NO IS NULL                                                                                " & vbNewLine _
