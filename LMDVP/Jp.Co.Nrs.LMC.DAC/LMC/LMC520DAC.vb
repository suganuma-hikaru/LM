    Private Sub SetConditionMasterSQL2()

        ' 追加条件設定
        Me._StrSql.Append(" AND(    OUTL.SYUBETU_KB <> '60'                                                " & vbNewLine)
        Me._StrSql.Append("     OR (OUTL.SYUBETU_KB =  '60' AND(OUTM.ALCTD_NB <> 0 OR OUTM.ALCTD_QT <> 0)) " & vbNewLine)
        Me._StrSql.Append("     )                                                                          " & vbNewLine)

    End Sub

